{
  "name": "vite-react-nestjs-electron",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "description": "Vite + React + Electron with Doubleshot, so fast! âš¡",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "rimraf dist && node scripts/copy-mail-templates.js && vite",
    "debug": "rimraf dist && vite -- --dsb-debug",
    "build": "rimraf dist && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@doubleshot/nest-electron": "^0.2.8",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.1.5",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/typeorm": "^11.0.0",
    "handlebars": "^4.7.8",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "^1.0.125",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "electron": "^37.2.4",
    "electron-builder": "^26.0.12",
    "eslint": "^9.32.0",
    "lint-staged": "^16.1.2",
    "rimraf": "^6.0.1",
    "rollup-plugin-copy": "^3.5.0",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-doubleshot": "^0.0.18",
    "vite-plugin-static-copy": "^3.1.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@nestjs/core",
      "@swc/core",
      "electron",
      "esbuild",
      "simple-git-hooks"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
